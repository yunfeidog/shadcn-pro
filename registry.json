      "dependencies": ["next-mdx-remote"],
      "files": [
        {
          "path": "registry/yunfei/blocks/markdown.tsx",
          "type": "registry:component"
        }
      ]
    },
    {
      "name": "data-table",
      "type": "registry:component",
