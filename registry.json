      "dependencies": ["@microsoft/fetch-event-source"],
      "files": [
        {
          "path": "registry/yunfei/hooks/use-ai.ts",
          "type": "registry:hook"
        }
      ]
    },
    {
      "name": "code-block",
      "type": "registry:component",
